{
  "name": "generate-function-bindings",
  "version": "0.0.0",
  "private": true,
  "description": "Tool to generate CDK for Terraform function bindings",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "watch-preserve-output": "tsc -w --preserveWatchOutput",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretest": "yarn build",
    "pretest:ci": "yarn build",
    "test": "yarn pretest && yarn lint && jest",
    "test:ci": "yarn pretest:ci && yarn lint && jest --ci",
    "jest-watch": "jest --watch",
    "package": "./package.sh",
    "dist-clean": "rm -rf dist",
    "fetch-metadata": "ts-node scripts/fetch-metadata.ts",
    "generate": "ts-node scripts/generate.ts"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/hashicorp/terraform-cdk.git",
    "directory": "tools/generate-function-bindings"
  },
  "author": {
    "name": "HashiCorp",
    "url": "https://hashicorp.com"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "cdktf": "0.0.0",
    "log4js": "^6.7.0",
    "prettier": "2.8.8"
  },
  "devDependencies": {
    "@babel/generator": "^7.20.4",
    "@babel/template": "^7.18.10",
    "@babel/types": "^7.20.2",
    "@cdktf/provider-generator": "0.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/prettier": "2.7.3",
    "ts-node": "^10.9.1",
    "typescript": "5.4.5"
  },
  "lint-staged": {
    "src/**/*.ts": "tsc-files ambient.d.ts --noEmit"
  }
}
