{
  "name": "cdktf-cli",
  "version": "0.0.0",
  "description": "Terraform for CDK CLI",
  "bin": {
    "cdktf": "bin/cdktf"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "test": "yarn lint && jest",
    "jest-watch": "jest --watch",
    "package": "./package.sh"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hashicorp/terraform-cdk-poc.git",
    "directory": "packages/cdktf-cli"
  },
  "author": {
    "name": "HashiCorp",
    "url": "https://hashicorp.com"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js"
    ]
  },
  "license": "MPL-2.0",
  "dependencies": {
    "cdktf": "0.0.0",
    "codemaker": "^0.22.0",
    "constructs": "^2.0.1",
    "fs-extra": "^8.1.0",
    "jsii": "1.1.0",
    "jsii-pacmak": "1.1.0",
    "semver": "^7.3.2",
    "sscaff": "^1.2.0",
    "yargs": "^15.1.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-use-before-define": 0
    },
    "ignorePatterns": [
      "node_modules",
      "dist",
      "coverage"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.1.2",
    "@types/json-schema": "^7.0.4",
    "@types/node": "13.7.0",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
