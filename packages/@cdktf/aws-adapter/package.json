{
  "name": "@cdktf/aws-adapter",
  "version": "0.0.0",
  "description": "Use AWS CDK constructs in CDKTF projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "echo 'this will soon be removed as the package moved'",
    "watch": "jsii -w --silence-warnings reserved-word",
    "watch-preserve-output": "tsc -w --preserveWatchOutput",
    "package": "jsii-pacmak",
    "package:python": "jsii-pacmak --targets python",
    "package:java": "jsii-pacmak --targets java",
    "package:dotnet": "jsii-pacmak --targets dotnet",
    "package:js": "jsii-pacmak --targets js",
    "package:go": "jsii-pacmak --targets go",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:examples": "lerna run --scope @cdktf/aws-adapter-example-* test",
    "dist-clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hashicorp/terraform-cdk.git",
    "directory": "packages/@cdktf/aws-adapter"
  },
  "author": {
    "name": "HashiCorp",
    "url": "https://hashicorp.com"
  },
  "jsii": {
    "outdir": "dist",
    "versionFormat": "short",
    "targets": {
      "python": {
        "distName": "cdktf-aws-adapter",
        "module": "cdktf_aws_adapter"
      },
      "java": {
        "package": "com.hashicorp.cdktf_aws_adapter",
        "maven": {
          "groupId": "com.hashicorp",
          "artifactId": "cdktf_aws_adapter"
        }
      },
      "dotnet": {
        "packageId": "HashiCorp.CdktfAwsAdapter",
        "namespace": "HashiCorp.CdktfAwsAdapter"
      },
      "go": {
        "moduleName": "github.com/hashicorp/terraform-cdk-aws-adapter-go",
        "packageName": "awsadapter"
      }
    },
    "excludeTypescript": ["examples"]
  },
  "license": "MPL-2.0",
  "dependencies": {
    "cdktf-cli": "^0.0.0",
    "change-case": "^4.1.1",
    "codemaker": "^1.34.0",
    "debug": "^4.3.2",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.0.0",
    "aws-cdk-lib": "^2.0.0-rc.17",
    "cdktf": "0.0.0",
    "jest": "^27.2.1",
    "jsii": "^1.29.0",
    "jsii-pacmak": "^1.29.0",
    "typescript": "^3.9.7",
    "ts-jest": "^27.0.5",
    "constructs": "^10.0.0",
    "@cdktf/hcl2json": "0.0.0"
  },
  "peerDependencies": {
    "constructs": "^10.0.0",
    "aws-cdk-lib": "^2.0.0-rc.17",
    "cdktf": "^0.0.0"
  },
  "bundledDependencies": ["cdktf-cli",
    "change-case",
    "codemaker",
    "debug",
    "fs-extra"]
}
