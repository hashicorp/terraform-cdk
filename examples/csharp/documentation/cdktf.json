{
  "language": "csharp",
  "app": "dotnet run -p MyTerraformStack.csproj",
  "terraformProviders": [
    "aws@~> 3.0",
    "kubernetes@~> 2.0",
    "hashicorp/random@~> 3.1",
    "dnsimple/dnsimple",
    "kreuzwerker/docker",
    "integrations/github"
  ],
  "terraformModules": [
    {
      "name": "vpc",
      "source": "terraform-aws-modules/vpc/aws",
      "version": "~> 3.0"
    },
    {
      "name": "eks",
      "source": "terraform-aws-modules/eks/aws",
      "version": "~> 3.0"
    },
    {
      "name": "my_local_module",
      "source": "./local_module/"
    }
  ],
  "context": {
    "excludeStackIdFromLogicalIds": "true"
  }
}
